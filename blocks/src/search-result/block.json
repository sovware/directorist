{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"category": "directorist-blocks-collection",
	"version": "1.0.0",
	"title": "Search Result",
	"name": "directorist/search-result",
	"description": "Create directory search result grid view, list view or map view.",
	"attributes": {
		"view":{
			"type":"string",
			"default":"grid"
		},
		"orderby":{
			"type":"string",
			"default":"date"
		},
		"order":{
			"type":"string",
			"default":"desc"
		},
		"listings_per_page":{
			"type":"number",
			"default":6
		},
		"show_pagination":{
			"type":"boolean",
			"default":false
		},
		"header":{
			"type":"boolean",
			"default":true
		},
		"header_title":{
			"type":"string",
			"default":"Search Result"
		},
		"columns":{
			"type":"number",
			"default":3
		},
		"featured_only":{
			"type":"boolean",
			"default":false
		},
		"popular_only":{
			"type":"boolean",
			"default":false
		},
		"logged_in_user_only":{
			"type":"boolean",
			"default":false
		},
		"map_height":{
			"type":"number",
			"default":500
		},
		"map_zoom_level":{
			"type":"number",
			"default":0
		}
	},
	"supports": {
		"html": false
	},
	"editorScript": "file:./index.js"    
}
